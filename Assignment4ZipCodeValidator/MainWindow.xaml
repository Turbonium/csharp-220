<Window x:Class="Assignment4ZipCodeValidator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment4ZipCodeValidator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Background="#FFD6C3C3">
        <TextBlock>Enter a Valid ZipCode:</TextBlock>
        <TextBox x:Name="uxZipCode" Background="#FFF0F0E4" TextChanged="uxZip_CodeEntered"></TextBox>
        <Button x:Name="uxSubmit" Click="uxSubmit_Click" Background="#FFF07373" Content="Please Enter a Valid ZipCode" IsEnabled="False"/>
    </StackPanel>
</Window>
